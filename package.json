{
  "name": "rn-expo-starter",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow bun",
    "prepare": "husky",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "lint": "eslint .",
    "lint:expo": "expo lint",
    "lint:fix": "eslint --fix .",
    "prettier:fix": "prettier '**/*.{js,jsx,ts,tsx,json,md}' --write",
    "start": "expo start",
    "start:no-cache": "expo start --clear",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "generate:color-token": "bun scripts/convert-colors.ts && prettier constant/color.ts --write && eslint constant/color.ts --fix",
    "test": "jest",
    "test:debug": "jest -o --watch --coverage=false",
    "test:dev": "jest --watchAll --coverage=false --changedSince=origin/main",
    "test:updateSnapshots": "jest -u --coverage=false",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@lottiefiles/dotlottie-react": "^0.13.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "expo": "^53.0.0-preview.12",
    "expo-constants": "~17.1.3",
    "expo-image": "~2.1.5",
    "expo-linking": "~7.1.3",
    "expo-localization": "~16.1.3",
    "expo-router": "~5.0.2-preview.6",
    "expo-secure-store": "~14.2.2",
    "expo-splash-screen": "~0.30.6",
    "expo-status-bar": "~2.2.2",
    "expo-system-ui": "~5.0.5",
    "expo-web-browser": "~14.1.5",
    "i18next": "^25.0.1",
    "lottie-react-native": "7.2.2",
    "lucide-react-native": "^0.503.0",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-i18next": "^15.1.1",
    "react-native": "0.79.1",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.3.0",
    "react-native-screens": "~4.10.0",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5",
    "sonner": "^2.0.3",
    "sonner-native": "^0.19.0",
    "tailwind-merge": "^3.0.1",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@commitlint/types": "^19.8.0",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22",
    "@types/react": "~19.0.10",
    "eslint": "^9.25.1",
    "eslint-config-expo": "~9.1.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-testing-library": "^7.1.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.0",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react-test-renderer": "19.0.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.3"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx}": "eslint --fix",
    "*.{js,css,md,ts,tsx,jsx}": "prettier --write"
  }
}
