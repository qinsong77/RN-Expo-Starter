{
  "name": "rn-expo-starter",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow bun",
    "prepare": "husky",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "lint": "eslint .",
    "lint:expo": "expo lint",
    "lint:fix": "eslint --fix .",
    "prettier:fix": "prettier '**/*.{js,jsx,ts,tsx,json,md}' --write",
    "start": "expo start",
    "start:no-cache": "expo start --clear",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "generate:color-token": "bun scripts/convert-colors.ts && prettier constant/color.ts --write",
    "test": "jest",
    "test:debug": "jest -o --watch --coverage=false",
    "test:dev": "jest --watchAll --coverage=false --changedSince=origin/main",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "expo": "^51.0.31",
    "expo-constants": "~16.0.2",
    "expo-linking": "~6.3.1",
    "expo-localization": "~15.0.3",
    "expo-router": "~3.5.23",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-web-browser": "~13.0.3",
    "i18next": "^23.14.0",
    "nativewind": "^4.0.36",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^15.0.1",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.2",
    "react-native-reanimated": "3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-web": "~0.19.10",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "^3.4.10",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@commitlint/types": "^19.0.3",
    "@testing-library/react-native": "^12.6.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "~18.2.79",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.1.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.8.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-testing-library": "^6.3.0",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "jest-expo": "~51.0.4",
    "lint-staged": "^15.2.2",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "react-test-renderer": "^18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx}": "eslint --fix",
    "*.{js,css,md,ts,tsx,jsx}": "prettier --write"
  }
}
