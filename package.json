{
  "name": "rn-expo-starter",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow bun",
    "prepare": "husky",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "lint": "eslint .",
    "lint:expo": "expo lint",
    "lint:fix": "eslint --fix .",
    "prettier:fix": "prettier '**/*.{js,jsx,ts,tsx,json,md}' --write",
    "start": "expo start",
    "start:no-cache": "expo start --clear",
    "android": "DARK_MODE=media expo start --android",
    "ios": "DARK_MODE=media expo start --ios",
    "web": "DARK_MODE=media expo start --web",
    "test": "jest",
    "test:debug": "jest -o --watch --coverage=false",
    "test:dev": "jest --watchAll --coverage=false --changedSince=origin/main",
    "test:updateSnapshots": "jest -u --coverage=false",
    "check-types": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx}": "eslint --fix",
    "*.{js,css,md,ts,tsx,jsx}": "prettier --write"
  },
  "dependencies": {
    "@better-auth/expo": "^1.3.4",
    "@expo/html-elements": "0.4.2",
    "@expo/vector-icons": "^14.1.0",
    "@gluestack-ui/button": "^1.0.14",
    "@gluestack-ui/checkbox": "^0.1.39",
    "@gluestack-ui/divider": "^0.1.10",
    "@gluestack-ui/form-control": "^0.1.19",
    "@gluestack-ui/icon": "^0.1.27",
    "@gluestack-ui/image": "^0.1.17",
    "@gluestack-ui/input": "^0.1.38",
    "@gluestack-ui/link": "^0.1.29",
    "@gluestack-ui/nativewind-utils": "^1.0.26",
    "@gluestack-ui/overlay": "^0.1.22",
    "@gluestack-ui/pressable": "^0.1.23",
    "@gluestack-ui/spinner": "^0.1.15",
    "@gluestack-ui/toast": "^1.0.9",
    "@hookform/resolvers": "^5.2.1",
    "@legendapp/motion": "^2.4.0",
    "@lottiefiles/dotlottie-react": "^0.13.5",
    "babel-plugin-module-resolver": "^5.0.2",
    "better-auth": "^1.3.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "expo": "^53.0.20",
    "expo-constants": "^17.1.7",
    "expo-image": "~2.4.0",
    "expo-linking": "^7.1.7",
    "expo-localization": "~16.1.6",
    "expo-router": "~5.1.4",
    "expo-secure-store": "~14.2.3",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "^14.2.0",
    "i18next": "^25.2.1",
    "lottie-react-native": "7.2.2",
    "lucide-react-native": "^0.525.0",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.61.1",
    "react-i18next": "^15.1.1",
    "react-native": "0.79.5",
    "react-native-css-interop": "^0.1.22",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5",
    "sonner": "^2.0.5",
    "sonner-native": "^0.21.0",
    "tailwind-merge": "^3.0.1",
    "tailwindcss": "3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@commitlint/types": "^19.8.0",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22",
    "@types/react": "~19.0.10",
    "eslint": "^9.30.",
    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-testing-library": "^7.5.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.9",
    "jscodeshift": "0.15.2",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "react-test-renderer": "19.0.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "enabled": true,
        "exclude": [
          "i18next",
          "@lottiefiles/dotlottie-react",
          "class-variance-authority",
          "sonner",
          "tailwindcss-animate"
        ],
        "listUnknownPackages": true
      }
    }
  }
}
